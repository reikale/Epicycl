<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - Tymespace</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/Epicycl.styles.css" asp-append-version="true" />
    @RenderSection("Styles", false)
    <link rel="stylesheet" href="~/lib/DataTables/datatables.min.css" asp-append-version="true" />
   
    
</head>
<body>
    <header>
        @Html.Partial("_NavBar")
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container text-disabled">
            &copy; @DateTime.Now.Year - Tymespace - <a asp-area="" asp-controller="Home" asp-action="Privacy" class="text-disabled">Privacy</a>
            
        </div>
    </footer>

    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="popper.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

     <script src="~/lib/bootbox/bootbox.min.js"></script>
    <script src="~/lib/bootbox/bootbox.locales.min.js"></script>
    <script src="~/lib/DataTables/datatables.min.js"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
